@model EditorViewModel
@{
    ViewBag.Title = "Radio - Add";
}
<h2>@ViewBag.Title</h2>
    <form asp-antiforgery="true" asp-controller="radio" asp-action="save">
        <div class="form-group">
            <label asp-for="Id">Id</label>
            <input type="text" class="form-control" asp-for="Id" />
        </div>

        <div class="form-group">
            <label asp-for="Name">Name</label>
            <input type="text" class="form-control" asp-for="Name" />
        </div>

        <div class="form-group">
            <label asp-for="Description">Description</label>
            <input type="text" class="form-control" asp-for="Description" />
        </div>

        <div class="form-group">
            <label asp-for="StreamUrl">StreamUrl</label>
            <input type="text" class="form-control" asp-for="StreamUrl" />
        </div>

        <div class="form-group">
            <label asp-for="StreamQuality">StreamQuality</label>
            <input type="text" class="form-control" asp-for="StreamQuality" />
        </div>

        <div class="form-group">
            <label asp-for="Genre">Genre</label>
            <input type="text" class="form-control" asp-for="Genre" />
        </div>

        <div class="form-group">
            <label asp-for="Priority">Priority</label>
            <input type="text" class="form-control" asp-for="Priority" />
        </div>


        <div class="form-group">
            <label asp-for="IsActive">IsActive?</label>
            <input type="text" class="form-control" asp-for="IsActive" />
        </div>

    </form>