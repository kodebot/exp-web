@model DetailViewModel
@{
    ViewData["Title"] = "Radio - Detail";
}

<h2>@ViewData["Title"]</h2>

<div>
    <dl class="dl-horizontal">
        <dt>Id:</dt>
        <dd>@Model.Id</dd>

        <dt>Name:</dt>
        <dd>@Model.Name</dd>

        <dt>Description:</dt>
        <dd>@Model.Description</dd>

        <dt>Genre:</dt>
        <dd>@Model.Genre</dd>

        <dt>Priority:</dt>
        <dd>@Model.Priority</dd>
    </dl>
</div>